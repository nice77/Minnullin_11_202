<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>com.example.demo.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>sayhi</servlet-name>
        <servlet-class>com.example.demo.SayHi</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sayhi</servlet-name>
        <url-pattern>/sayhi</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>hi</servlet-name>
        <servlet-class>com.example.demo.Hi</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hi</servlet-name>
        <url-pattern>/hi</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>calc</servlet-name>
        <servlet-class>com.example.demo.Calc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>calc</servlet-name>
        <url-pattern>/calc</url-pattern>
    </servlet-mapping>
</web-app>